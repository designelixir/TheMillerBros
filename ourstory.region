<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <link rel="stylesheet" href="https://use.typekit.net/ctq0biw.css">
    <link rel="stylesheet" href="styles/our-story.less">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes} texture">

    <div class="site-container">
      <header class="site-header">
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />
      </header>

      <div style="width: 100%; height: 60vh; min-height: 400px; border: 2px solid magenta; background-image: url(assets/18.jpg); background-size: cover; background-position: center center; background-repeat: no-repeat;">
  {#  header photo  }
      </div>
      <div style="width: 100%; height: 40vh; border: 4px solid aqua; margin-top: -20vh; display: flex; align-items: flex-end; justify-content: center;  ">
        <h6 style="text-align: center; color: white; font-size: 3rem;"> Telling stories from the path less traveled.</h6>
      </div>


    <div class="section">
      <div id="review-wrapper">
          <h1>&rdquo;</h1>
          <squarespace:block-field id="our-story-review" columns="1"/>
          <h1>&rdquo;</h1>
      </div>
        <div id="review-name-wrapper" >
          <span style='width: 100px; height: 100px;'><squarespace:block-field id="our-story-review-img" columns="1"/></span>
          <span style="padding: 25px 0 0 10px;">
            <squarespace:block-field id="our-story-reviewer" columns="1"/>
            <squarespace:block-field id="our-story-reviewer-company" columns="1"/>
          </span>
      </div>
      </div>

      <div class="bio-container">
        {.repeated section items}
        <div class="bio-wrapper inset-box-shadow" style="background-image: url({assetUrl}?format=original); ">
            <div class="bio-content-wrapper">
                <span class="bio-content drop-shadow">
                {body}
                <img class="signature-img" src="{customContent.signature.assetUrl}"/>
                </span>
            </div>
        </div>
        {.end}
        </div>

        <div class="inset-box-shadow" style="width: 100%; padding: 100px 50px;">
            <squarespace:block-field id="team-description" columns="1"/>
        </div>


      <main class="content-container" id="our-story-content" role="main" data-content-field="main-content" >
       {squarespace.main-content}
      </main>
    </div>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>
<script>



</script>
