
<div class="homepage-wrapper">

<!-- item loop -->
{.repeated section items}



<div class="featured-wrapper">
    <div class="slide-video" >

        <div class='embed-container' id="post-{id}" class="portfolio-item {@|item-classes}" data-item-id="{id}" style="background-image: url('{fullURL}?format=original'); background-repeat: no-repeat;">
            <iframe src={'https://player.vimeo.com/video/'+ '565081064' + '?autoplay=1&loop=1&background=1'} frameborder='0' webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe> 
        </div>

    </div>

    <div class="featured-info">
        <h1>{title}</h1>
    </div>

</div>
    

{.or}

  <!-- no items -->
  <p><em>No blog posts yet.</em></p>

{.end}


</div>
<script src="https://player.vimeo.com/api/player.js"></script>
<script>
var form = $('.form_element').val();
var el = $('a#Link').prop('href');

el.prop('href', el.prop('href')+form);
</script>